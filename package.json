{
  "name": "evel-react-cli",
  "version": "0.0.2",
  "description": "this is node cli for react project",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "start": "ts-node bin/yoso.ts",
    "test": "jest --config test/jest-config.json",
    "prepublishOnly": "ncc build bin/yoso.ts -o dist",
    "build:clear": "find ./actions ./bin ./commands ./utils ./ui ./component -type f -name '*.d.ts' -delete & find ./actions ./bin ./commands ./utils ./ui ./component -type f -not -name '*.ts*' -delete",
    "commit": "git-cz .",
    "release": "standard-version",
    "push": "git push --follow-tags origin master && npm publish",
    "doc": "typedoc --out doc"
  },
  "bin": {
    "evel_react_cli": "dist/index.js"
  },
  "type": "module",
  "keywords": [
    "cli",
    "react",
    "cli",
    "node",
    "cli"
  ],
  "author": "evel",
  "license": "ISC",
  "devDependencies": {
    "@types/ini": "^1.3.30",
    "@types/progress": "^2.0.3",
    "@types/react": "^16.9.49",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.2"
  },
  "dependencies": {
    "@types/node": "^14.6.3",
    "commander": "^6.1.0",
    "ini": "^1.3.5",
    "ink": "^3.0.5",
    "ink-text-input": "^4.0.0",
    "log-symbols": "^4.0.0",
    "nunjucks": "^3.2.2",
    "ora": "^5.1.0",
    "progress": "^2.0.3",
    "react": "^16.13.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/EvalGitHub/evel_react_cli.git"
  },
  "bugs": {
    "url": "https://github.com/EvalGitHub/evel_react_cli/issues"
  },
  "homepage": "https://github.com/EvalGitHub/evel_react_cli#readme"
}
