{
  "name": "evel-react-cli",
  "version": "0.0.2",
  "description": "this is node cli for react project",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "tsc",
    "start": "ts-node bin/yoso.ts",
    "prepublish": "npm run build",
    "postpublish": "npm run build:clear",
    "build:clear": "find ./actions ./bin ./commands  -type f -name '*.d.ts' -delete & find ./actions ./bin ./commands  -type f -not -name '*.ts*' -delete"
  },
  "bin": {
    "yoso": "bin/yoso.js"
  },
  "keywords": [
    "cli",
    "react",
    "cli",
    "node",
    "cli"
  ],
  "author": "evel",
  "license": "ISC",
  "devDependencies": {
    "ts-node": "^9.0.0"
  },
  "dependencies": {
    "@types/node": "^14.6.3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/EvalGitHub/evel_react_cli.git"
  },
  "bugs": {
    "url": "https://github.com/EvalGitHub/evel_react_cli/issues"
  },
  "homepage": "https://github.com/EvalGitHub/evel_react_cli#readme"
}
